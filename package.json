{
  "name": "@rwsnl/wkx",
  "version": "1.0.0",
  "description": "A WKT/WKB/EWKT/EWKB/TWKB/GeoJSON parser and serializer",
  "main": "dist/wkx.mjs",
  "types": "dist/wkx.d.ts",
  "exports": {
    "default": {
      "types": "./lib/wkx.d.ts",
      "import": "./lib/wkx.mjs"
    }
  },
  "author": "RWS-NL",
  "license": "MIT",
  "scripts": {
    "test": "yarn build && mocha -r esm",
    "build": "rollup -c",
    "postinstall": "yarn build",
    "prepack": "yarn build && pinst --disable",
    "postpack": "pinst --enable",
    "coveralls": "istanbul cover node_modules/mocha/bin/_mocha -- -r esm -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "dependencies": {
    "@types/node": "*"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "async": "^2.6.0",
    "coveralls": "^3.0.1",
    "deep-eql": "^3.0.1",
    "esm": "^3.0.84",
    "istanbul": "^0.4.5",
    "json-stringify-pretty-compact": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.1.1",
    "pg": "^7.4.3",
    "rollup": "^0.66.2",
    "rollup-plugin-copy": "^3.4.0",
    "terser": "^3.8.2"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/cschwarz/wkx.git"
  },
  "keywords": [
    "wkt",
    "wkb",
    "ewkt",
    "ewkb",
    "twkb",
    "geojson",
    "ogc",
    "geometry",
    "geography",
    "spatial"
  ],
  "packageManager": "yarn@3.6.0"
}
