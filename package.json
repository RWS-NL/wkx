{
	"name": "@rws-nl/wkx",
	"version": "1.0.0",
	"description": "A WKT/WKB/EWKT/EWKB/TWKB/GeoJSON parser and serializer",
	"main": "dist/wkx.mjs",
	"module": "dist/wkx.mjs",
	"types": "dist/wkx.d.mts",
	"type": "module",
	"exports": {
		"types": "./dist/index.d.mts",
		"import": "./dist/index.mjs"
	},
	"author": "RWS-NL",
	"license": "MIT",
	"scripts": {
		"lint": "eslint src test --ext mjs,mts --fix",
		"format": "prettier --write .",
		"test": "mocha",
		"build": "rollup -c",
		"postinstall": "husky install .github/husky",
		"prepack": "yarn build && pinst --disable",
		"postpack": "pinst --enable"
	},
	"dependencies": {
		"@types/node": "*"
	},
	"devDependencies": {
		"@babel/core": "^7.22.1",
		"@babel/eslint-parser": "^7.21.8",
		"@babel/plugin-syntax-import-assertions": "^7.20.0",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^4.0.0",
		"deep-eql": "^4.1.3",
		"eslint": "^8.42.0",
		"eslint-config-neon": "^0.1.47",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"mocha": "^10.2.0",
		"pinst": "^3.0.0",
		"prettier": "^2.8.8",
		"pretty-quick": "^3.1.3",
		"rollup": "^3.23.1",
		"rollup-plugin-copy": "^3.4.0"
	},
	"files": [
		"dist/"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RWS-NL/wkx.git"
	},
	"bugs": {
		"url": "https://github.com/RWS-NL/wkx/issues"
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,mts,js"
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.8"
	},
	"prettier": "@sapphire/prettier-config",
	"keywords": [
		"wkt",
		"wkb",
		"ewkt",
		"ewkb",
		"twkb",
		"geojson",
		"ogc",
		"geometry",
		"geography",
		"spatial"
	],
	"packageManager": "yarn@3.6.0"
}
