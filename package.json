{
	"name": "@rws-nl/wkx",
	"version": "1.0.0",
	"description": "A WKT/WKB/EWKT/EWKB/TWKB/GeoJSON parser and serializer",
	"main": "dist/wkx.mjs",
	"types": "dist/wkx.d.ts",
	"exports": {
		"default": {
			"types": "./lib/wkx.d.ts",
			"import": "./lib/wkx.mjs"
		}
	},
	"author": "RWS-NL",
	"license": "MIT",
	"scripts": {
		"lint": "eslint src test --ext js,ts --fix",
		"format": "prettier --write .",
		"test": "mocha -r esm",
		"build": "rollup -c",
		"postinstall": "husky install .github/husky",
		"prepack": "yarn build && pinst --disable",
		"postpack": "pinst --enable"
	},
	"dependencies": {
		"@types/node": "*"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.4.2",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^4.0.0",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"deep-eql": "^3.0.1",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"esm": "^3.0.84",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"mocha": "^5.1.1",
		"pinst": "^3.0.0",
		"prettier": "^2.8.8",
		"pretty-quick": "^3.1.3",
		"rollup": "^0.66.2",
		"rollup-plugin-copy": "^3.4.0"
	},
	"files": [
		"dist/"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RWS-NL/wkx.git"
	},
	"bugs": {
		"url": "https://github.com/RWS-NL/wkx/issues"
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.8"
	},
	"prettier": "@sapphire/prettier-config",
	"keywords": [
		"wkt",
		"wkb",
		"ewkt",
		"ewkb",
		"twkb",
		"geojson",
		"ogc",
		"geometry",
		"geography",
		"spatial"
	],
	"packageManager": "yarn@3.6.0"
}
