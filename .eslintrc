{
	"extends": "@sapphire",
	"rules": {
		"@typescript-eslint/prefer-for-of": "off",
		"@typescript-eslint/explicit-member-accessibility": "off",
		"@typescript-eslint/no-invalid-this": "off",
		"block-scoped-var": "off",
		"func-names": "off",
		"no-buffer-constructor": "off",
		"no-eval": "off",
		"no-negated-condition": "off",
		"no-var": "off",
		"prefer-spread": "off",
		"radix": "off"
	}
}
